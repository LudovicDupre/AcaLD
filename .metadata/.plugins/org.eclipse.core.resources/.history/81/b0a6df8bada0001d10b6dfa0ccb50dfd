import java.util.Scanner;

public class Unite {
	
	static Scanner sc = new Scanner(System.in);
	
	public static void main(String[] args) {
		

	}
	public static double foot(int x) {
		return  x*3.28084;
	}
	public static double pound(int y) {
		return y*453.592;
	}
	public static double fahren(int z) {
		return (z*(9/5)+32);
	}

}
