					{1,3,4}};
		
		
		
	}
	public static int[][] iteratingMatrix(int[][] mat1, int[][] mat2){
		int[][] answer = new int[mat1.length][mat2[0].length];
		for (int row = 0; row<mat1.length; row++) {
			for (int column = 0; column <mat2[0].length; column++) {
		}
		return  answer[row][column];
	}
}}